{
  "name": "My Bookmark Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "storage.html",
    "default_icon": "hello_extensions.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "bookmarks",
    "history"
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+L",
        "mac": "Command+Shift+L"
      },
      "description": "Open the Bookmark Manager"
    }
  }
}